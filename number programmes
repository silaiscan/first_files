#biggest number programme
x = 0
for i in range(10):
    number=int(input(str(i+1) + " . number :"))
    if number > x:
       x = number
print("the biggest number =" , x)

#positive number programme
total = 0
for i in range(15):
    number=int(input(str(i+1) + ". number : " ))
    if (number >= 0):
        total += 1
print("The sum  : ", total)

#prime number programme
number=int(input("enter the number:"))
for i in range(2,number//2 + 2):
    if number % i ==0: break
if i >= number/2:
    print("prime number")
else:
    print("not prime number")


#prime number method 2
number=int(input(" enter a number :"))
for i in range(2,int(number**0.5)+2):
    if number %i ==0: break
if i > number**0.5:
    print("prime number")
else:
    print("not prime number")

#fibonacci numbers programme
liste=[]
adet=int(input("kaç adet fibonacci sayısı görüntülemek istiyorsunuz ? :"))
a,b,i=0,1,1
if adet==1:
    liste.append(a)
    print(liste)
else:
    liste.insert(0,0)
    while i<=adet:
        liste.append(b)
        a,b=b,a+b
        i=i+1
    print(liste)

limit=int(input("hangi sayıya kadar olan fibonacci sayılarını görüntülemek istiyorsunuz ? :"))
a,b=0,1
print(a)
while b<limit:
    print(b)
    a,b=b,a+b

#percentage programme
a = int(input("Enter a number: "))
b = int(input("Enter another number: "))

percentage = float(a*(b/100))
print("%", percentage , sep='')

#largest number programme
a=int(input("enter a number :"))
b=int(input("enter b number :"))
c=int(input("enter c number :"))

if a > b and a > c :
    print("a is the largest number")
elif b > c:
    print("b is the largest number")
else:
    print("c is the largest number")
